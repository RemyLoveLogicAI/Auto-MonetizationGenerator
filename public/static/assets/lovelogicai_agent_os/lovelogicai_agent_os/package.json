{
  "name": "lovelogicai-agent-os",
  "version": "1.0.0",
  "description": "Revolutionary AI Agent Operating System with persistent digital consciousness",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace LoveLogicAIAgentOS.xcworkspace -scheme LoveLogicAIAgentOS -configuration Release -destination generic/platform=iOS -archivePath LoveLogicAIAgentOS.xcarchive archive",
    "dev": "concurrently "npm run start" "npm run backend:dev"",
    "backend:dev": "cd ../core && python -m uvicorn main:app --reload --port 8000",
    "test:integration": "detox test",
    "test:e2e": "maestro test e2e/",
    "postinstall": "cd ios && pod install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-native-async-storage/async-storage": "^1.19.3",
